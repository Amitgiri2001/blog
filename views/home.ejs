<%- include("partials/header") %>
    <h1>Home</h1>
    <p>
        <%=content %>
    </p>

    <% posts.forEach(function(post){ %>
        <% console.log(post.postTitle)  %>
        <h1>
            <%= post.postTitle %>
        </h1>
        <p>
            <%= post.postBody.slice(0,100) %>
            <a href="/posts/<%= post.postTitle  %>">...ReadMore</a>
        </p>
    <% }) %>

<%- include("partials/footer") %>